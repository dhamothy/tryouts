.{$pre_global}-{$footer}{
	padding: 30px 0 60px;
	font-size: 12px;
	color: $color_muted;
	
	&__{
		&container{
			display: flex;
			justify-content: space-between;
		}
		&item{
			color: $color_muted;
			text-transform: uppercase;
			margin-left: 12px
			font-size: 10px;
			
			&:hover{
				color: $color_primary;
			}
		}
	}
}